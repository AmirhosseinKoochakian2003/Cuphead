<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import javafx.scene.text.Text?>
<BorderPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="view.Scoreboard"
            prefHeight="400.0" prefWidth="600.0"
            stylesheets="@css/scoreboard.css">
<center>
    <VBox alignment="CENTER" spacing="10">
        <Text text="scoreboard" styleClass="name"/>
        <GridPane fx:id="gridPane" alignment="CENTER" />
    <Button text="show" onMouseClicked="#showList" fx:id="button"/>
    <Button text="back" fx:id="back" onMouseClicked="#backClicked"/>
    </VBox>
</center>
</BorderPane>
